<div class="container">
    <h1>Kontingenter</h1>
    <div class="row">
        <div class="col">
            <form method="post" action="/subscriptionType">
                <div class="form-group">
                    <label for="subscriptionInfoInput">Kontingent information</label>
                    <input type="subscriptionInfo" name = "subInfo" class="form-control" placeholder="Kontigent navn">
                </div>
                <div class="form-group">
                    <label for="subLenghtInput">Kontingent længde</label>
                    <input type="subLenght" name = "subLenght" class="form-control" placeholder="Kontingent længde">
                </div>
                <div class="form-group">
                    <label for="subPriceInput">Kontingent pris</label>
                    <input type="subPrice" name = "subPrice" class="form-control" placeholder="Månedlig pris">
                </div>
                <button type="submit" class="btn btn-primary">Opret</button>
            </form>
        </div>
    </div>
</div>
<div class="row">
    <ul class="list-group">
        <li class="subscriptions"> </li>
    </ul>
</div>